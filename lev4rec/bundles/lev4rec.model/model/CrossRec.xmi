<?xml version="1.0" encoding="UTF-8"?>
<lowcoders:RSModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:lowcoders="https://it.disim.univaq/lowcode"
    xsi:schemaLocation="https://it.disim.univaq/lowcode lowcoders.ecore"
    name="CrossRec">
  <dataset xsi:type="lowcoders:SupervisedDataset"
      name="crossrec"
      path="results.csv">
    <indipendentVariables
        name="User"/>
    <indipendentVariables
        name="Item"/>
    <dataStructure
        xsi:type="lowcoders:Matrix"
        name="RatingsMatrix"
        columns="//@dataset/@indipendentVariables.0 //@dataset/@indipendentVariables.1"/>
    <datasetManipulationLibrary>PANDAS</datasetManipulationLibrary>
    <dependatVariable
        name="Rating"/>
  </dataset>
  <presentationLayer
      xsi:type="lowcoders:WebApplication"
      name="FlaskApp"
      port="5000"
      host="0.0.0.0"/>
  <evaluation>
    <validationTechnique
        xsi:type="lowcoders:ContextValidation"
        name="recommend list"
        nOfRecommendations="10"
        testContext="//@recommendationSystem/@context"/>
  </evaluation>
  <recommendationSystem
      xsi:type="lowcoders:FilteringRS"
      name="KNN"
      generator="SURPRISE"
      cutoff="10"
      neighborhood="40">
    <context
        name="UserContext"
        userContext="//@dataset/@dataStructure"/>
  </recommendationSystem>
  <feedback>
    <expressedFeedback
        xsi:type="lowcoders:Rating">
      <item
          outcame="//@dataset/@indipendentVariables.1"/>
    </expressedFeedback>
  </feedback>
</lowcoders:RSModel>

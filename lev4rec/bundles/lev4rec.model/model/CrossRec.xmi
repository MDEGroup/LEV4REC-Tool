<?xml version="1.0" encoding="UTF-8"?>
<lowcoders:RSModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:lowcoders="https://it.disim.univaq/lowcode"
    xsi:schemaLocation="https://it.disim.univaq/lowcode lowcoders.ecore"
    name="CrossRec">
  <dataset xsi:type="lowcoders:UnsupervisedDataset">
    <indipendentVariables
        name="item"/>
    <dataStructure
        xsi:type="lowcoders:Graph"/>
  </dataset>
  <recommendationSystem
      xsi:type="lowcoders:MachineLearningBasedRS">
    <scope/>
  </recommendationSystem>
  <feedback>
    <query userContext="//@dataset/@dataStructure"/>
    <expressedFeedback
        xsi:type="lowcoders:ExplicitFeedback"
        item="//@recommendationSystem/@scope"/>
  </feedback>
</lowcoders:RSModel>

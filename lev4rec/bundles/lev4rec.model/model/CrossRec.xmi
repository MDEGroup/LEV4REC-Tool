<?xml version="1.0" encoding="UTF-8"?>
<lowcoders:RSModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:lowcoders="https://it.disim.univaq/lowcode"
    xsi:schemaLocation="https://it.disim.univaq/lowcode lowcoders.ecore"
    name="CrossRec">
  <dataset xsi:type="lowcoders:SupervisedDataset"
      name="crossrec"
      path="ml-100k"
      isBuiltIn="true">
    <datasetManipulationLibrary>PANDAS</datasetManipulationLibrary>
  </dataset>
  <presentationLayer
      xsi:type="lowcoders:WebApplication"
      name="FlaskApp"
      port="5000"
      host="0.0.0.0"/>
  <evaluation>
    <metrics>PRECISION</metrics>
    <metrics>RECALL</metrics>
    <metrics>F1_MEASURE</metrics>
    <validationTechnique
        xsi:type="lowcoders:CrossValidation"
        nOfRecommendations="10"
        library="SURPRISE"
        numberOfFold="10"/>
  </evaluation>
  <recommendationSystem
      xsi:type="lowcoders:FilteringRS"
      name="KNN"
      generator="SURPRISE"
      cutoff="10"
      neighborhood="40">
    <context
        name="UserContext"/>
  </recommendationSystem>
</lowcoders:RSModel>
